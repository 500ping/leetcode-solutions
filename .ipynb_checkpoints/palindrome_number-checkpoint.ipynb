   "source": [
    "def isPalindrome(x: int) -> bool:\n",
    "    if x < 0:\n",
    "        return False\n",
    "    \n",
    "    x_to_str = str(x)\n",
    "    \n",
    "    return x_to_str == x_to_str[::-1]"
   ]
